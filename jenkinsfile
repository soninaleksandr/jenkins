pipeline {
    agent any
    stages {
        stage('Be polite') {
            steps {
                script {
                    echo "Hello I am your new job !"
                    
                    sh label: '', script: '''sh \'env > env.txt\' 
                    for (String i : readFile(\'env.txt\').split("\\r?\\n")) {
                        println i
                    }'''
                }
            }
        }
    }
}
